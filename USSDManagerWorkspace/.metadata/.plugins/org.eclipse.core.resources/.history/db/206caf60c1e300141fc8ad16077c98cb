package za.co.olamide.ussd;

@XmlRootElement()
public class USSDTransferElement 
	{
	@XmlElement()
	public String type;
	@XmlElement()
	public int itemIndex;
	@XmlElement()
	public String text;
	@XmlElement()
	public String actionType;
	@XmlElement()
	public String actionValue;
	@XmlElement()
	public String nextMenuName;
	}	
